            <kind>1</kind>
            <string>//Arrow keys movement
if (keyboard_check(ord("A"))) {
    x -= spd;
    if (image_xscale &gt; 0) {
        image_xscale = -image_xscale;
    }
}
if (keyboard_check(ord("D"))) {
    x += spd;
    if (image_xscale &lt; 0) {
        image_xscale = -image_xscale;
    }
}
if (keyboard_check(ord("W")))y -= spd;
if (keyboard_check(ord("S")))y += spd;

//Boundaries
if (x - (spriteRes_x/2) &lt;= 0) {
    x = spriteRes_x/2;
}
if (y - spriteRes_y/2 &lt;= 0) {
    y = spriteRes_y/2;
}
if (x + spriteRes_x/2 &gt;= room_width) {
    x = room_width - spriteRes_x/2;
}
if (y + spriteRes_y/2 &gt;= room_height) {
    y = room_height - spriteRes_y/2;
}

//GROW DAMNIT
if (keyboard_check(vk_up)) {
    image_yscale *= 1.1;
    if (image_xscale &gt; 0) {
        image_xscale *= 1.1;
    }
    if (image_xscale &lt; 0) {
        image_xscale *= 1.1;
    }
}
if (keyboard_check(vk_down)){
    image_yscale /= 1.1;
    if (image_xscale &gt; 0) {
        image_xscale /= 1.1;
    }
    if (image_xscale &lt; 0) {
        image_xscale /= 1.1;
    }
}

//Shooting
cooldown -= 1;
if (mouse_check_button(mb_left) &amp;&amp; cooldown &lt;= 0) {
    instance_create(x + (30 * image_xscale), y - (19 * image_yscale), objBullet);
    audio_play_sound(sndGunshot, 10, false);
    cooldown = 10;
}

//Destroy self and restart game if hp runs out
if (hp &lt;= 0) {
    game_restart();
}
</string>
